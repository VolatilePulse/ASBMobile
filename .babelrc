{
   "presets": [
      [
         "env",
         {
            "modules": false,
            "targets": {
               "browsers": [
                  "> 2%",
                  "last 2 versions",
                  "not ie <= 8"
               ]
            }
         }
      ],
      "stage-2"
   ],
   "plugins": [
      "transform-runtime"
   ],
   "env": {
      "test": {
         "presets": [
            "env",
            "stage-2"
         ],
         "plugins": [
            "istanbul"
         ]
      }
   }
}
